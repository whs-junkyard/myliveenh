{
	"name": "chatsound",
	"description": "เล่นเสียง chat",
	"category": "Chat",
	"private": true,
	"content_scripts": [
		{
			"matches": ["http://mylive.in.th/streams/*", "http://mylive.in.th/chat/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],

	"settings": {
		"chatSoundVolume": {
			"default": 0.25,
			"type": "number",
			"label": "Volume (0-1)",
			"step": 0.05
		}
	}
}
